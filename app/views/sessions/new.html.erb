
  <div class="container">
    <h1>Log In</h1>
    <%= form_tag create_path do %>
    
    <div class="field">
      <%= label_tag :email %>
      <%= text_field_tag :email, params[:email] %>
    </div>
    <div class="field">
      <%= label_tag :password %>
      <%= password_field_tag :password %>
    </div>
    <div class="action">
      <%= submit_tag :Submit %>
    </div>
    <% end %>
  </div>
