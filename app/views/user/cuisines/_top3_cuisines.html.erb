<h1>Top Cuisines</h1>
<div class="cats">

  <% current_user.top_cats.each do |cat| %>
      <table class="cats-list">
      <tr>
        <td>
          <%= cat[0] %>
        </td>
        <td>
          <%= button_tag "Find Restaurants Near Me", class:"nearby-restaurants", data: {cuisinelabel:  "#{@cuisines.find_by(name: cat[0]).label}"}  %>
          <%= button_tag "Find Dindr Date!", class:"dindr-dates", data: {cuisinename: cat[0]} %>
        </td>
      </tr>
    </table>
  <% end %>
</div>
